  "keywords": ["blog", "gatsby", "photography", "react"],
  "license": "MIT",
  "lint-staged": {
    "*.js": ["prettier --write", "git add -f"],
    "*.json": ["prettier --write", "git add"],
    "*.css": ["prettier --write", "git add"],
    "src/pages/posts/**/*.md": ["yarn lint:text", "git add"]
  },
  "main": "n/a",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/jessestuart/jessestuart.com"
  },
  "scripts": {
    "build": "yarn build:s3-plugin && gatsby build",
    "build:ci": "GATSBY_ENV=ci yarn build",
    "build:s3-plugin": "cd ./plugins/gatsby-source-s3 && yarn && cd ../../",
    "clean": "rm -rf node_modules/ .cache/ && yarn -s",
    "dev":
      "GATSBY_ENV=ci NODE_OPTIONS=--max_old_space_size=4096 gatsby develop",
    "dev:photos":
      "NODE_OPTIONS=--max_old_space_size=4096 concurrently 'yarn watch:s3-plugin' 'yarn gatsby develop'",
    "flow:install": "flow-typed -o -l ./flow-libs/ install",
    "format": "prettier 'src/**/*.js' 'src/**/*.md'",
    "lint": "concurrently 'yarn lint:js' 'yarn lint:style'",
    "lint:js":
      "eslint --format 'node_modules/eslint-friendly-formatter' --quiet --ext .js .",
    "lint:style": "stylelint 'src/**/*.css'",
    "lint:stylefix": "stylelint --fix 'src/**/*.css'",
    "lint:text": "textlint src/pages/**/index.md",
    "lint:textfix": "textlint --fix src/pages/**/index.md",
    "precommit": "lint-staged",
    "snyk-protect": "snyk protect",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "update": "ncu -ua && yarn -s",
    "watch:s3-plugin": "yarn build:s3-plugin -w",
    "willitpass": "concurrently 'yarn lint' 'yarn test'"
  },
  "snyk": true
}
