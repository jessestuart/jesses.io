  },
  "scripts": {
    "build": "gatsby build",
    "clean": "rm -rf node_modules/ .cache/ && yarn install -q",
    "dev": "gatsby develop",
    "format": "prettier 'ssrc/**/*.js' 'src/**/*.md'",
    "lint": "concurrently 'yarn lint:js' 'yarn lint:style'",
    "lint:js":
      "eslint --format 'node_modules/eslint-friendly-formatter' --quiet --ext .js .",
    "lint:style": "stylelint 'src/**/*.(css|scss)'",
    "lint:stylefix": "stylelint --fix 'src/**/*.(css|scss)'",
    "lint:text": "textlint src/pages/**/index.md",
    "lint:textfix": "textlint --fix src/pages/**/index.md",
    "precommit": "lint-staged",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:coveralls":
      "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "test:coveralls-alt": "cat ./test/coverage/lcov.info | yarn coveralls",
    "test:snapshots:skip": "env MODE='skip-snapshots' jest",
    "test:snapshots:update": "jest -u",
    "test:watch": "jest --watch"
  }
