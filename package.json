  "scripts": {
    "build": "GATSBY_ENV=Production gatsby build",
    "build:ci": "GATSBY_ENV=Staging gatsby build",
    "check-types": "tsc -p tsconfig.json",
    "clean": "rm -rf node_modules/ .cache/ && yarn -s",
    "cloc": "cloc --vcs=git --by-file-by-lang --not-match-f=yarn.lock .",
    "dev": "GATSBY_ENV=Development NODE_OPTIONS=--max_old_space_size=4096 gatsby develop",
    "dev:photos": "NODE_OPTIONS=--max_old_space_size=4096 concurrently 'yarn gatsby develop'",
    "e2e:percy": "percy exec ts-node ./test/e2e/run-percy-cypress.js",
    "format": "prettier 'src/**/*.js' 'src/**/*.md'",
    "lint": "concurrently 'yarn lint:js' 'yarn lint:style' 'yarn check-types'",
    "lint:js": "eslint --cache --quiet --ext .ts,.tsx ./src/",
    "lint:style": "stylelint 'src/**/*.css'",
    "lint:stylefix": "stylelint --fix 'src/**/*.css'",
    "lint:text": "textlint src/pages/**/index.md",
    "lint:textfix": "textlint --fix src/pages/**/index.md",
    "test": "JEST_JUNIT_OUTPUT=reports/junit/js-test-results.xml jest",
    "test:ci": "JEST_JUNIT_OUTPUT='reports/junit/js-test-results.xml' yarn jest --coverage --runInBand --ci",
    "test:coverage": "yarn test --coverage",
    "test:update-snapshots": "yarn test --updateSnapshot",
    "test:watch": "yarn test --watch",
    "update": "ncu -u && yarn -s",
    "willitpass": "concurrently 'yarn lint' 'yarn test'"
  }
